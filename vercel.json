{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "public-react",
  "framework": null,
  "functions": {
    "api/*.ts": {
      "maxDuration": 10,
      "includeFiles": "build/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}